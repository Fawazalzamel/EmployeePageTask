@{
}

@model List<Employee>




    <table class="table">
        <thead>
            <td>Employee Number</td>
            <td>Employee First Name</td>
            <td>Employee Last Name</td>
            <td>Employee Department</td>
        </thead>

        <tbody>
        @foreach (var emloyeeDetails in Model)
        {
            <tr>
                
                <td>@emloyeeDetails.EmployeeId</td>
                <td>@emloyeeDetails.FirstName</td>
                <td>@emloyeeDetails.LastName</td>
                <td>@emloyeeDetails.Department</td>
            </tr>
        }
        </tbody>  
    </table>
        



